	<queryString>
		<![CDATA[select (select dbo.number((select sum(amount) from tbFabricsIndent where pono=a.pono and supplierId=b.supplierid),b.dolar)) as Taka,b.indentId,' ' as ShippingMarks,isnull(sc.StyleNo,'') as StyleNo, isnull(fi.ItemName,'') as AccessorisItem,  ISNULL(c.Colorname,'') as ColorName,isnull(brand.name,'') as brandName,'' as accessoriesSize,'' as size,
 (select unitname from tbunits where UnitId=b.UnitId) as UnitName,b.width,b.GSM,b.TotalQty,b.TotalQty as RequireUnitQty,b.sqNumber,b.skuNumber,b.dolar,b.rate,b.dolar,b.amount,b.sampleQty,a.currency,b.poManual,a.orderDate,deliveryDate,
 (select MerchendiserName from TbMerchendiserInfo  where MerchendiserId=a.orderby) OrderBy,
 ei.contact as MerMobile,
 ei.email as MerEmail,a.Note,a.Subject,cast(a.body as varchar(300)) as body,a.ManualPo,
 (select Signature from Tblogin where id=b.entryby) as Signature,
 (select Signature from Tblogin where id='9') as MdSignature,
 (select organizationLogo from tbOrganizationInfo where organizationId = 1) organizationLogo,
 b.mdapproval,a.poNo,a.caNo,a.contentNo,a.fabricsContent,a.terms
 from tbPurchaseOrderSummary a
 join tbFabricsIndent b
 on a.pono=b.pono
 left join TbStyleCreate sc
 on b.styleId = cast(sc.StyleId as varchar)
 left join TbFabricsItem fi
 on b.fabricsid = cast(fi.id as varchar)
 left join tbColors c
 on b.fabricscolor = cast(c.ColorId as varchar)
left join tbbrands brand
on b.brand = brand.id
left join Tblogin l
on a.entryBy = l.id
left join tbEmployeeInfo ei
on l.employeeId = ei.autoId
 where a.pono='3' and a.supplierid='7'
 order by  b.styleid,b.PurchaseOrder,b.Itemid,b.fabricsid asc]]>
	</queryString>
